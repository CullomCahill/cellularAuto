{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/style.css",
      "dest": "/public/style.css"
    },
    {
      "src": "/cellAuto.js",
      "dest": "/public/cellAuto.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/server.js"
    }
  ]
}